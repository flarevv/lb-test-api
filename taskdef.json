{
    "containerDefinitions": [ 
       { 
          "command": null,
          "entryPoint": null,
          "essential": true,
          "image": "lb-test-api",
          "logConfiguration": { 
             "logDriver": "awslogs",
             "options": { 
                "awslogs-group" : "/ecs/fargate-task-definition",
                "awslogs-region": "us-east-1",
                "awslogs-stream-prefix": "ecs"
             }
          },
          "name": "lb-test-ap",
          "portMappings": [ 
            { 
               "containerPort": 3000,
               "hostPort": 3000,
               "protocol": "tcp"
            }
         ]
       }
    ],
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::657028987194:role/ecsTaskExecutionRole",
    "family": "lb-test-api",
    "memory": "512",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [ 
        "FARGATE" 
     ]
 }